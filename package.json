		"pony-cause": "^2.1.4",
		"type-fest": "^4.0.0"
	},
	"devDependencies": {
		"@types/glob": "8.0.0",
		"@types/lodash": "4.14.186",
		"@types/uuid": "8.3.4",
		"@types/verror": "1.10.6",
		"@vitest/coverage-v8": "^0.34.6",
		"@vitest/ui": "^0.34.6",
		"del": "^5.0.0",
		"del-cli": "5.0.0",
		"eslint": "8.57.0",
		"eslint-plugin-mmkal": "0.5.1",
		"execa": "^5.1.1",
		"expect-type": "0.14.2",
